{
  "root": true,
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "parser": "babel-eslint"
  },
  "rules": {
    "eqeqeq": "warn",
    "multiline-ternary": ["warn", "always-multiline"]
  }
}